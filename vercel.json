{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "functions": {
        "app.py": {
            "includeFiles": "nanobanana_app/**"
        }
    }
}